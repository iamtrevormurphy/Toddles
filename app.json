{
  "expo": {
    "name": "Toddles",
    "slug": "toddles",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFF8E7"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.toddles.app",
      "requireFullScreen": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#FFF8E7"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.toddles.app"
    },
    "web": {
      "favicon": "./assets/favicon-32.png",
      "bundler": "metro",
      "name": "Toddles",
      "shortName": "Toddles",
      "description": "Educational games for toddlers",
      "backgroundColor": "#FFF8E7",
      "themeColor": "#4ECDC4"
    },
    "extra": {
      "ageRange": "2-6",
      "category": "educational"
    }
  }
}
